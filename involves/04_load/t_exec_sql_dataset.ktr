    <sql>/* 
 * Criação da Tabela Dataset
 */

CREATE TABLE IF NOT EXISTS dw.ft_dados_coleta(
	id_dataset varchar(4) PRIMARY KEY,
	id_pdv varchar(3) NOT NULL,
	nome_pdv varchar(14),
	perfil_pdv varchar(9),
	id_linha_produto varchar(3) NOT NULL,
	nome_linha_produto varchar(18),
	marca_linha_produto varchar(13),
	data_coleta date NOT NULL,
	dia_coleta bigint,
	mes_coleta bigint,
	ano_coleta bigint,
	CONSTRAINT fk_id_dataset FOREIGN KEY (id_dataset) REFERENCES ft_coleta(id_dataset) ON DELETE CASCADE,
	CONSTRAINT fk_pdv_dataset FOREIGN KEY (id_pdv) REFERENCES dim_pdv(id_pdv) ON DELETE CASCADE,
	CONSTRAINT fk_produto_dataset FOREIGN KEY (id_linha_produto) REFERENCES dim_linha_produto(id_linha_produto) ON DELETE CASCADE
) ENGINE=INNODB;

DELETE dataset
FROM dw.ft_dados_coleta dataset
INNER JOIN dw.ft_coleta ft
	ON dataset.id_dataset = ft.id_dataset;

INSERT IGNORE INTO dw.ft_dados_coleta
	SELECT 
		coleta.id_dataset,
		coleta.id_pdv,
		pdv.nome_pdv,
		pdv.perfil_pdv,
		coleta.id_linha_produto,
		produto.nome_linha_produto,
		produto.marca_linha_produto,
		coleta.data_coleta,
		calendario.dia_coleta,
		calendario.mes_coleta,
		calendario.ano_coleta
	FROM dw.ft_coleta coleta
	INNER JOIN dw.dim_pdv pdv ON coleta.id_pdv = pdv.id_pdv
	INNER JOIN dw.dim_linha_produto produto ON coleta.id_linha_produto = produto.id_linha_produto
	INNER JOIN dw.dim_calendario calendario ON coleta.data_coleta = calendario.data_coleta;</sql>
