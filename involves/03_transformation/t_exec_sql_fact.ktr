INNER JOIN stg.data_import stg
	ON ft.id_linha_produto = stg.ID_LINHA_PRODUTO;	
	
INSERT IGNORE INTO dw.ft_ponto_extra
	SELECT 
		ID_LINHA_PRODUTO AS id_linha_produto,
		SUM(VALOR) AS qtd_ponto_extra
	FROM stg.data_import
	WHERE TIPO_COLETA = 'PONTO EXTRA'
		AND MONTH(STR_TO_DATE(`DATA`, '%d/%m/%y')) = '09'
		AND YEAR(STR_TO_DATE(`DATA`, '%d/%m/%y')) = '2020'
	GROUP BY ID_LINHA_PRODUTO;</sql>
