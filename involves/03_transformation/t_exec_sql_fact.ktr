CREATE TABLE IF NOT EXISTS dw.ft_ponto_extra(
	id_linha_produto varchar(14) PRIMARY KEY,
	qtd_presenca_agregada bigint NOT NULL,
	CONSTRAINT fk_linha_produto_ponto_extra FOREIGN KEY (id_linha_produto) REFERENCES dim_linha_produto(id_linha_produto) ON DELETE CASCADE
) ENGINE=INNODB;

DELETE ft
