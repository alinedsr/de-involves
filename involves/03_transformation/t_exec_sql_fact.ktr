    <sql>/*  
*	Fato Disponibilidade Agregada (FT_DISPONIBILIDADE_AGREGADA):
*	Deve conter os ids de ligação das tabelas de dimensões (Dimensão
*	Calendário e Ponto de Venda) e a quantidade de presença de linhas de
*	produto agrupadas por ponto de venda no mês de Setembro/20.
 */

CREATE TABLE IF NOT EXISTS dw.ft_disponibilidade_agregada(
	id_disponibilidade varchar(50) PRIMARY KEY,
	id_pdv varchar(3) NOT NULL,
	id_linha_produto varchar(14) NOT NULL,
	qtd_presenca_agregada bigint NOT NULL,
	CONSTRAINT fk_pdv FOREIGN KEY (id_pdv) REFERENCES dim_pdv(id_pdv) ON DELETE CASCADE,
	CONSTRAINT fk_linha_produto_agregada FOREIGN KEY (id_linha_produto) REFERENCES dim_linha_produto(id_linha_produto) ON DELETE CASCADE
) ENGINE=INNODB;

INSERT IGNORE INTO dw.ft_disponibilidade_agregada
	SELECT 
		CONCAT(stg.ID_PONTO_VENDA, stg.ID_LINHA_PRODUTO) AS id_disponibilidade,
		stg.ID_PONTO_VENDA AS id_pdv,
		stg.ID_LINHA_PRODUTO AS id_linha_produto,
		COUNT(stg.ID_LINHA_PRODUTO) AS qtd_presenca_agregada
	FROM stg.data_import AS stg
	LEFT JOIN dw.ft_disponibilidade_agregada AS ft 
		ON stg.ID_PONTO_VENDA = ft.id_pdv
			AND stg.ID_LINHA_PRODUTO = ft.id_linha_produto
		WHERE stg.VALOR = 'SIM'
			AND MONTH(STR_TO_DATE(stg.`DATA`, '%d/%m/%y')) = '09'
			AND YEAR(STR_TO_DATE(stg.`DATA`, '%d/%m/%y')) = '2020'
			AND ft.id_disponibilidade NOT IN (SELECT DISTINCT ft.id_disponibilidade FROM dw.ft_disponibilidade_agregada)
	GROUP BY stg.ID_PONTO_VENDA, stg.ID_LINHA_PRODUTO;</sql>
