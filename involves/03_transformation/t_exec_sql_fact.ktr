    <sql>/*  
*	Não foi solicitado, mas estou adicionando uma fato com as informações de coleta, para criação do dataset final.
 */

CREATE TABLE IF NOT EXISTS dw.ft_coleta(
	id_dataset varchar(4) PRIMARY KEY,
	id_pdv varchar(3) NOT NULL,
	id_linha_produto varchar(14) NOT NULL,
	data_coleta date NOT NULL,
	tipo_coleta varchar(15) NOT NULL,
	CONSTRAINT fk_pdv_coleta FOREIGN KEY (id_pdv) REFERENCES dim_pdv(id_pdv) ON DELETE CASCADE,
	CONSTRAINT fk_linha_produto_coleta FOREIGN KEY (id_linha_produto) REFERENCES dim_linha_produto(id_linha_produto) ON DELETE CASCADE,
	CONSTRAINT fk_data FOREIGN KEY (data_coleta) REFERENCES dim_calendario(data_coleta) ON DELETE CASCADE
) ENGINE=INNODB;

INSERT IGNORE INTO dw.ft_coleta
	SELECT 
		ID_DATASET AS id_coleta,
		ID_PONTO_VENDA AS id_pdv,
		ID_LINHA_PRODUTO id_linha_produto,
		STR_TO_DATE(`DATA`, '%d/%m/%y') AS data_coleta,
		TIPO_COLETA AS tipo_coleta
	FROM stg.data_import
WHERE NOT EXISTS (SELECT DISTINCT id_dataset FROM dw.ft_coleta);</sql>
