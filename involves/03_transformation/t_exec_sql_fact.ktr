    <sql>/*  
*	Fato Disponibilidade (FT_DISPONIBILIDADE): Deve conter os ids de
*	ligação das tabelas de dimensões criadas na questão anterior e a
*	quantidade de presenças de cada linha de produto no mês de
*	Setembro/20. 
 */

CREATE TABLE IF NOT EXISTS dw.ft_disponibilidade(
	id_linha_produto varchar(14)PRIMARY KEY,
	qtd_presenca bigint NOT NULL,
	CONSTRAINT fk_linha_produto FOREIGN KEY (id_linha_produto) REFERENCES dim_linha_produto(id_linha_produto) ON DELETE CASCADE
) ENGINE=INNODB;

INSERT IGNORE INTO dw.ft_disponibilidade
	SELECT 
		stg.ID_LINHA_PRODUTO AS id_linha_produto,
		COUNT(stg.ID_LINHA_PRODUTO) AS qtd_presenca
	FROM stg.data_import AS stg
	LEFT JOIN dw.ft_disponibilidade AS ft 
		ON stg.ID_LINHA_PRODUTO = ft.id_linha_produto
		WHERE stg.VALOR = 'SIM'
			AND MONTH(STR_TO_DATE(stg.`DATA`, '%d/%m/%y')) = '09'
			AND YEAR(STR_TO_DATE(stg.`DATA`, '%d/%m/%y')) = '2020'
			AND ft.id_linha_produto NOT IN (SELECT DISTINCT ft.id_linha_produto FROM dw.ft_disponibilidade)
	GROUP BY stg.ID_LINHA_PRODUTO;</sql>
